FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p0;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];


internalField   uniform 0;

boundaryField
{
   top
    {
         type            calculated;
         value           $internalField;
    }
    bottom
    {
         type            calculated;
         value           $internalField;
    }
    left
    {
         type            calculated;
         value           $internalField;
    }
    right
    {
         type            calculated;
         value           $internalField;
    }
    frontandback
    {
         type            empty;
    }
}


// ************************************************************************* //
